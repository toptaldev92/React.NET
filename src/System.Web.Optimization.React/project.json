  "copyright": "Copyright 2014-Present Facebook, Inc",
  "description": "Allows you to transpile JavaScript via Babel in the ASP.NET Web Optimization Framework.",

  "buildOptions": {
    "compile": {
      "includeFiles": [
        "../SharedAssemblyInfo.cs"
      ]
    },
    "keyFile": "../key.snk",
